{
  "name": "geo-aggr-basa",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node local",
    "start:prod": "ts-node prod",
    "init-test-data": "ts-node test_utils/init_osm_test_data.ts",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix",
    "type": "tsc",
    "test:ci": "jest --passWithNoTests",
    "test": "jest --watchAll --coverage",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec \"ts-node\" local",
    "client": "ts-node test_utils/ws_client",
    "d:build": "docker build -t basa .",
    "d:run": "docker run -p 9090:8000 -e PORT='8000' basa",
    "d:prod": "docker run -p 8000:8000 -e PORT='8000' basa",
    "d:start": "npm run d:build && npm run d:prod"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/inversify": "^2.0.33",
    "@types/kdbush": "^3.0.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.25",
    "@types/pg": "^7.14.11",
    "@types/socket.io": "^2.1.13",
    "@types/stats-lite": "^2.2.0",
    "@types/ws": "^7.4.0",
    "bson": "^4.2.2",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "inversify": "^5.0.5",
    "kdbush": "^3.0.0",
    "knex": "^0.95.4",
    "morgan": "~1.9.1",
    "osm-extractor": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "stats-lite": "^2.2.0",
    "telegraf": "^4.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.0"
  }
}